{
    "configurations": {
        "run": {
            "adapter": "debugpy",
            "default": "true",
            "configuration": {
                "request": "launch",
                "python": "${workspaceRoot}/../../../venv/bin/python",
                "cwd": "${workspaceRoot}",
                "stopOnEntry": false,
                "program": "${workspaceRoot}/parse_script.py"
            },
            "breakpoints": {
                "exception": {
                    "raised": "N",
                    "uncaught": "",
                    "userUnhandled": ""
                }
            }
        }
    }
}
